<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <div class="board">
      <ul>
        <% imgs.forEach(function (img) { %>
          <li>
            <img src="<%= img.filename %>" class="img">
            <p><%= img.filename %></p>
          </li>
        <% }) %>
      </ul>
    </div>
  </body>
</html>
